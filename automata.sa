start{
    'a'..'z' | _ => identifier_state
    '0'..'9' => number_state
    "Self" =>  return KEYWORD_SELF
}

identifier_state{
    'a'..'z' | '0'..'9' | _  => Self
    _ => return IDENTIFIER
}

number_state{
    '0'..'9' => Self
    '.' => float_state
    _ => return NUMBER
}

float_state{
    '0'..'9' => Self
    _ => return FLOAT
}
